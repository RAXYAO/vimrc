" Default Bundles {

    " Use before config if available {
        if filereadable(expand("$VIM/.vimrc.before"))
            source $VIM/.vimrc.before
        endif
    " }

    " Use bundles config {
        if filereadable(expand("$VIM/.vimrc.plug"))
            source $VIM/.vimrc.plug
        endif
    " }
" }
